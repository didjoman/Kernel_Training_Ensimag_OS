#ifndef PROCESS_PRIORITY_QUEUE_H
#define PROCESS_PRIORITY_QUEUE_H

#include "uptime.h"
#include "process.h"
#include "stdbool.h"

struct priority_queue{
        struct Process* head;
	struct Process* tail;
};

void insert(struct proc_sleep_queue* queue, struct Process* p);
struct Process* pop(struct proc_sleep_queue* queue, time_t get_up);
bool empty(struct proc_sleep_queue* queue);


#endif

